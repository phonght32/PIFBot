<?xml version="1.0" ?>
<sdf version="1.4">
  <model name="robot_thesis">
    <static>false</static>

    <link name="robot_base">
      <collision name="collision">
        <pose>0 0 .1025 0 0 0</pose>
        <geometry>
          <box>
            <size>.2 .2 .14</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0 0 .0625 0 0 -1.5707</pose>
        <geometry>
          <mesh>
            <uri>model://robot_thesis/meshs/dae/robot_base.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <collision name="caster_front_collision">
        <pose>-0.06 0 0.0325 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.0325</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <!-- <visual name="caster_front_visual">
        <pose>0.06 0 0.0325 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.0325</radius>
          </sphere>
        </geometry>
      </visual> -->
      <collision name="caster_behind_collision">
        <pose>0.06 0 0.0325 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.0325</radius>
          </sphere>
        </geometry>
        <!-- <visual name="caster_front_visual">
        <pose>0.06 0 0.0325 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.0325</radius>
          </sphere>
        </geometry>
      </visual> -->
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <link name="robot_leftwheel">
      <pose>0 0.09 0.0325 0 1.5707 1.5707</pose>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>.0325</radius>
            <length>.026</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0 0 0 -1.5707 0  0  </pose>
        <geometry>
          <mesh>
            <uri>model://robot_thesis/meshs/dae/robot_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="robot_rightwheel">
      <pose>0 -0.09 0.0325 0 1.5707 1.5707</pose>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>.0325</radius>
            <length>.026</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0 0 0 1.5707 0 0</pose>
